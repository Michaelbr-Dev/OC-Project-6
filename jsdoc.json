{
  "tags": {
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./", "README.md"],
    "includePattern": ".js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/better-docs",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "search": true,
    "better-docs": {
      "name": "Piiquante API documentation",
      "title": "Piiquante API documentation",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/Michaelbr-Dev/OC-Project-6"
        }
      ]
    }
  }
}